from tkinter import Tk, BOTH, Canvas

class Window:
    def __init__(self, width, height):
    self.width = width
    self.height = height
    self.root = TK()
    self.root.title("Maze Solver")

    self.running = False

    self.canvas = Canvas(self.root, width=width, height=height)
    self.canvas.pack(fill=BOTH, expand=True, padx=10, pady=10)
